.celebrityview {
    width: 100%;
    background-color: white;
    padding-bottom: 24rpx;
    padding-top: 24rpx;
}

.celebrityview .titleview {
    display: flex;
    flex-direction: row;
    padding: 0 28rpx;
}

.celebrityview .titleview .titlelbl {
    color: #333333;
    font-size: 14px;
    font-weight: 700;
    margin-right: auto;
}

.celebrityview .titleview .alllbl {
    color: #969696;
    font-size: 12px;
    margin-right: 10rpx;
}

.celebrityview .cardsview {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin: 24rpx 28rpx 0 28rpx;
}

.celebrityview .cardsview .cardview {
    width: 200rpx;
}

.celebrityview .cardsview .cardview .adimage {
    width: 100%;
    height: 200rpx;
    display: flex;
    flex-direction: column;
}

.celebrityview .cardsview .cardview .titlelbl {
    color: #333333;
    font-size: 14px;
    margin-top: 26rpx;
    word-break: break-all;    
    text-overflow: ellipsis;    
    display: -webkit-box;    
    -webkit-box-orient: vertical;    
    -webkit-line-clamp: 1;    
    overflow: hidden; 
}

.celebrityview .cardsview .cardview .commonlbl {
    font-size: 12px;
    color: #969696;
    margin-top: 10rpx;
}