<template name="imagesview">
    <view class="imagesview">
        <block wx:for="{{images}}">
            <image class="imageview" id="{{index}}" src="{{item.square}}" mode="scaleToFill" bindload="imageLoaded" 
             style="width: {{item.width}}px; height:{{item.height}}px; margin-left:{{item.left}}px; margin-right:{{item.right}}px;"></image>
        </block>
        <view class="bottomview"></view>
    </view>
</template>